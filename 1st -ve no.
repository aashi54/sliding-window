#include <bits/stdc++.h>
using namespace std;

int main()
{
    int size = 5;
    int arr[5] = {-8, 2, 3, -6, 10};
    int k =2;
    
    int i=0, j=0;
    list<int> l;
    vector<int> ans;
   
    while(j<size){
        
         if(arr[j]<0){
             l.push_back(arr[j]);
             
         }
        
         if(j-i+1<k){
             j++;
         }
        
         else if(j-i+1 == k){
             ans.push_back(l.front());
             l.pop_front();
             i++;
             j++;
             
             
     }
        
    }  
    return 0;
    
    
    
    
    ///////////////////
    
    #include <bits/stdc++.h>
using namespace std;

int main()
{
    int size = 5;
    int arr[5] = {-8, 2, 3, -6, 10};
    int k =2;
    
    int i=0, j=0;
    queue<int> q;
    vector<int> v2;
   
    while(j<size){
        
         if(arr[j]<0){
             q.push(arr[j]);
             
         }
        
         if(j-i+1<k){
             j++;
         }
        
         else if(j-i+1 == k){
                if(q.empty()){
                    v2.push_back(0);
                }
                else{
             v2.push_back(q.front());
             q.pop();
             i++;
             j++;}
             
             
     }
        
    }  
             for(int p =0; p<v2.size(); p++){
                 cout<<v2[p]<<endl;
             }
    return 0;
}
}
