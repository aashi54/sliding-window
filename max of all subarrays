#include <bits/stdc++.h>
using namespace std;

int main()
{
   int size = 8;
   int arr[8] = {1,3,-1,-3,5,3,6,7};
   int k = 3;
   
   list<int> l;
   vector<int> ans;
   
   int i=0, j=0;
   
  // write edge case of if k>size of arr
   
   while(j<size){
       while(l.size()>0 && l.back()<arr[j]){
           l.pop_back();
       }
       l.push_back(arr[j]);
       
       if(j-i+1 < k)
         j++;
         
         if(j-i+1 ==k){
           ans.push_back(l.front());
              
              if(l.front()==arr[i])
                 l.pop_front();
                  
                 i++;
                 j++;
         }
         
   }
      for(int k=0; k<ans.size(); k++)
          cout<<ans[k]<<endl;
      
    return 0;
}
